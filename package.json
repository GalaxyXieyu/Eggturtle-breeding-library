{
  "name": "eggturtle-node-rebuild",
  "private": true,
  "packageManager": "pnpm@9.15.5",
  "scripts": {
    "lint": "pnpm -r lint",
    "build": "pnpm -r build",
    "format": "prettier --write .",
    "api-tests": "pnpm --filter @eggturtle/api exec ts-node --project tsconfig.json ../../scripts/api-tests/run.ts",
    "api-tests:matrix": "pnpm api-tests -- --only account-matrix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "prettier": "^3.5.2",
    "typescript": "^5.8.2"
  }
}
